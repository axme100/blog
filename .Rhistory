data("df_mxmunicipio")
targetCodes <- df_mxmunicipio %>%
filter(state_name_official %in% c("Distrito Federal", "México")) %>%
select(municipio_code, municipio_name)
mapData <- left_join(chihuahuaData, targetCodes, by="municipio_name")
# Change names of data
mapData <- mapData %>%
rename(value=population,
region=municipio_code) %>%
select(region, value)
